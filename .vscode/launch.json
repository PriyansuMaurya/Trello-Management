{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run application",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "runtimeExecutable": "vite",
      "runtimeArgs": [],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "http://localhost:3000",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*"
      }
      
    }
  ]
}